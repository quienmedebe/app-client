{
  "name": "app-client",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "check-package-manager": "node -e \"!process.env.npm_config_user_agent.startsWith('npm/') && !console.log('---- USE NPM TO INSTALL DEPENDENCIES ----') && process.exit(1)\"",
    "preinstall": "npm run check-package-manager --silent",
    "clean": "npx react-native-clean-project",
    "build:android": "react-native run-android",
    "build:ios": "react-native run-ios",
    "build:all": "npm run build:android; npm run build:ios",
    "ios:pods": "cd ios && pod install; cd ..",
    "beta:android": "cd ./android && fastlane beta --env development",
    "beta:ios": "cd ./ios && fastlane beta --env development",
    "beta:all": "npm run beta:android && npm run beta:ios",
    "release:android": "cd ./android && fastlane release --env production",
    "release:ios": "cd ./ios && fastlane release --env production",
    "release:all": "npm run release:android && npm run release:ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-picker/picker": "^1.9.7",
    "@react-navigation/drawer": "^5.11.4",
    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.12.8",
    "ajv": "^7.0.2",
    "ajv-formats": "^1.5.1",
    "color": "^3.1.3",
    "react": "^17.0.2",
    "react-error-boundary": "^3.1.0",
    "react-hook-form": "^6.13.1",
    "react-native": "^0.64.0",
    "react-native-awesome-alerts": "^1.4.2",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-get-random-values": "^1.5.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.3",
    "react-native-paper": "^4.5.0",
    "react-native-picker-select": "^8.0.4",
    "react-native-progress": "^4.1.2",
    "react-native-reanimated": "^1.13.2",
    "react-native-restart": "0.0.20",
    "react-native-restart-android": "0.0.7",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.16.1",
    "react-native-splash-screen": "^3.2.0",
    "react-native-vector-icons": "^7.1.0",
    "react-redux": "^7.2.2",
    "realm": "^10.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/runtime": "^7.13.10",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "@types/react-native": "^0.64.2",
    "@types/react-redux": "^7.1.16",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^25.5.1",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.3.4",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.10.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.65.2",
    "pretty-quick": "^3.1.0",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  }
}
